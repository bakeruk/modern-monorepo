{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", "tsconfig.tsbuildinfo"]
    },
    "clean:build": {},
    "dev": {
      "persistent": true
    },
    "lint": {},
    "lint:fix": {},
    "format": {},
    "format:fix": {},
    "test": {
      "dependsOn": ["build"]
    },
    "typecheck": {
      "outputs": ["tsconfig.tsbuildinfo"]
    }
  }
}
